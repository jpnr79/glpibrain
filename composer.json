{
    "name": "glpibrain/plugin-project",
    "type": "plugin",
    "description": "Neural network prediction for ticket resolution and classification",
    "keywords": ["glpi", "plugin", "neuralnetworks", "machinelearning", "prediction", "classification", "ticket", "resolution", "incident"],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=8.1",
        "rindow/rindow-clblast-ffi": "^1.0",
        "rindow/rindow-math-matrix": "^2.0.4",
        "rindow/rindow-math-matrix-matlibffi": "^1.0",
        "rindow/rindow-neuralnetworks": "^2.1",
        "sabre/event": "^5"
    },
    "require-dev": {
        "glpi-project/tools": "^0.4",
        "rindow/rindow-math-plot": "~1.0.9",
        "ext-pdo_sqlite": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "GLPI\\Brain\\": "src/"
        }
    }
}
